  "devDependencies": {
    "@babel/preset-typescript": "7.3.3",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "babel-loader": "8.0.6",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "prettier": "1.18.2",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-node-externals": "1.7.2"
